name: dating_app
description: flutter dating_app project
publish_to: none
version: 1.0.0+1
environment: {sdk: '>=2.12.0 <3.0.0'}
dependencies:
  flutter: {sdk: flutter}
  cupertino_icons: ^1.0.2
  firebase_analytics: ^8.3.0
  firebase_messaging: ^10.0.5
  flutter_local_notifications: ^8.1.1+1
  equatable: ^2.0.3
  firebase_core: ^1.10.6

  image: ^3.0.4
  cached_network_image: ^3.1.0

  google_sign_in: ^5.2.1
  lottie: ^1.0.1
  crypto: ^3.0.1
  convert: ^3.0.0
  fluttertoast: ^8.0.7
  flutter_screenutil: ^5.0.0+2
  dio_cookie_manager: ^2.0.0
  flutter_svg: ^0.23.0+1
  get: ^4.6.1
  # Get Storaget
  get_storage: ^2.0.3

  #Intl
  intl: ^0.17.0

  #Json Annotation
  json_annotation: ^4.3.0
  json_serializable: ^6.0.1

  #Network
  dio: ^4.0.0
  mime: ^1.0.0
  http_parser: ^4.0.0
  pull_to_refresh: ^2.0.0
  image_picker: ^0.8.3+3
  flutter_image_compress: ^1.1.0
  path_provider: ^2.0.3
  flutter_slidable: ^0.6.0
  scrollable_positioned_list: ^0.2.0-nullsafety.0

  #chat
  stream_chat_flutter: 3.3.2
  stream_chat_flutter_core: 3.3.1
  stream_chat_persistence: ^3.0.0
  stream_chat_localizations: ^2.0.0
  badges: ^2.0.1

  form_field_validator: ^1.1.0
  url_launcher: ^6.0.12
  flutter_native_splash: ^1.3.2
  cloud_firestore: ^3.1.5

dev_dependencies:
  flutter_test: {sdk: flutter}
  lints: ^1.0.1
  flutter_lints: ^1.0.0
  flutter_launcher_icons: ^0.9.2
  flutter_native_image: ^0.0.6+1
  build_runner: ^2.1.1



flutter:

  # The following line ensures that the Material Icons font is
  # included with your application, so that you can use the icons in
  # the material Icons class.
  uses-material-design: true

  # To add assets to your application, add an assets section, like this:
  assets:
    - assets/icons/
    - assets/images/
  fonts:
    - family: SFUIDisplay
      fonts:
        - asset: assets/fonts/sf-ui-display-thin.ttf
          weight: 100
        - asset: assets/fonts/sf-ui-display-light.ttf
          weight: 300
        - asset: assets/fonts/sf-ui-display-normal.ttf
          weight: 400
        - asset: assets/fonts/sf-ui-display-medium.ttf
          weight: 500
        - asset: assets/fonts/sf-ui-display-semibold.ttf
          weight: 600
        - asset: assets/fonts/sf-ui-display-bold.ttf
          weight: 700
        - asset: assets/fonts/sf-ui-display-heavy.ttf
          weight: 800
        - asset: assets/fonts/sf-ui-display-black.ttf
          weight: 900
